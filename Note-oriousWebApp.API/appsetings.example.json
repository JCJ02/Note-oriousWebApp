{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=<YOUR_HOST>;Port=<YOUR_PORT>;Database=<YOUR_DATABASE_NAME>;Username=<YOUR_USERNAME>;Password=<YOUR_PASSWORD>;"
  },
  "JwtSettings": {
    "SecretKey": "<YOUR_SECRETKEY>",
    "RefreshKey": "<YOUR_REFRESHKEY>",
    "ExpiresIn": 900,
    "RefreshExpiresIn": 604800
  }
}